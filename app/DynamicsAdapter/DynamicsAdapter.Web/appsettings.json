{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug",
        "DynamicsAdapter.Web.Health.DynamicsHealthCheck":  "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console"  }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Dynadapter"
    }
  },
  "OAuth": {
    "ResourceUrl": "",
    "ClientId": "",
    "Secret": "",
    "OAuthUrl": "",
    "Username": "",
    "Password": ""
  },
  "ApiGateway": {
    "BasePath": ""
  },
  "Scheduler": {
    "Cron": "0/15 * * * * ?"
  },
  "SearchApi": {
    "BaseUrl": "http://localhost:5050"
  },
  "OptionsSet": {
    "BaseUrl": "http://localhost:5050"
  }
}
