{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Splunk" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error",
        "DynamicsAdapter.Web.Health.DynamicsHealthCheck": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {SearchRequestKey}{DataPartner}{AgencyCode}{RequestRef}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Dynadapter"
    }
  },
  "OAuth": {
    "ResourceUrl": "http://test/resource",
    "ClientId": "test",
    "Secret": "test",
    "OAuthUrl": "http://test/resource",
    "Username": "test",
    "Password": "test"
  },
  "ApiGateway": {
    "BasePath": ""
  },
  "Scheduler": {
    "Cron": "0/45 * * * * ?",
    "Failed": "0 30 11 * * ? *",
    "ResultUpload": "0/2 * * ? * * *"
  },
  "SearchApi": {
    "BaseUrl": "http://localhost:5050"
  },
  "OptionsSet": {
    "BaseUrl": "http://localhost:5050"
  },
  "Redis": {
    "Password": "",
    "ConnectTimeout": 6000,
    "ConnectRetry": 2,
    "Hosts": [
      {
        "Host": "localhost",
        "Port": "6379"
      }
    ]
  },
  "AgencyNotification": {
    "WebHooks": [
      {
        "Name": "RequestApi",
        "Uri": "http://localhost:4000/api/Notification"
      }
    ]
  }
}
